# 开发环境配置
# NODE_ENV=development

# API配置 - 开发环境使用本地后端服务
VITE_API_BASE_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3000

# 功能开关 - 开发环境禁用模拟数据，使用真实WebSocket
VITE_USE_MOCK_DATA=true
VITE_ENABLE_DEBUG=true
VITE_ENABLE_LOG=true

# 用户配置 - 开发环境模拟用户
VITE_MOCK_USER_ID=test-user-001
VITE_MOCK_USER_TOKEN=dev-token-123456
VITE_MOCK_USER_NAME=测试用户

# 聊天配置
VITE_DEFAULT_GROUP_ID=group-001
VITE_DEFAULT_PRIVATE_CHAT_ID=private-001